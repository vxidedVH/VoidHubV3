local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "VoidHub", HidePremium = true, SaveConfig = true,IntroText ="VH" })

OrionLib:MakeNotification({
	Name = "VoidHub",
	Content = "Hi! "..Player.Name..". Thanks for using VoidHub, if you enconuter any bugs send them to VoidedScripts#8479 ",
	Image = "rbxassetid://4483345998",
	Time = 10
})

